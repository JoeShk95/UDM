             VLocked[xvehicles] = 0;
             SetPrice(xvehicles);
			 strmid(VehicleName[xvehicles], VehiclesName[GetVehicleModel(xvehicles)-400], 0, strlen(VehiclesName[GetVehicleModel(xvehicles)-400]), 255);
