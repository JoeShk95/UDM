forward Drift(playerid,cd,playerid2);
public Drift(playerid,cd,playerid2) {
if(playerid2 < 0) //only if playerid2 hasn't joined
{
for(new i=0;i<MAX_PLAYERS;i++) {
	if(IsPlayerConnected(i) && isinDrift[i] && i != playerid)
	{
	    playerid2 =i;
	}
  }
}
if(!isinDrift[playerid] || (playerid2 != -1 && !isinDrift[playerid2])) //מפעיל הדריפט פרש והמצטרף לא
{
TogglePlayerControllable(playerid,true);
TogglePlayerControllable(playerid2,true);
return 1;
}
if(playerid2 != -1 && !isinDrift[playerid2])
{

	if(IsPlayerConnected(playerid) && isinDrift[playerid])
	{
	SendClientMessage(playerid,0x0F482FF,".ניצחת טכנית מכיוון שיריבך פרש"); //מצטרף הדריפט פרש
 	LACY_GiveMoney(playerid,WinnerP);
  	isinDrift[playerid] = false;
   	drift[playerid] = 0;
    DisablePlayerRaceCheckpoint(playerid);
	}

	modeldrift = 0;
	Drifton = false;
	return 1;
	}


new Dstring[256];
format(Dstring,sizeof(Dstring),"~y~%d",cd);
if(IsPlayerConnected(playerid))
	GameTextForPlayer(playerid,Dstring,1000,6);
if(IsPlayerConnected(playerid2) && playerid2 != -1)
    GameTextForPlayer(playerid2,Dstring,1000,6);
if(cd < 1)
{
	OnDriftStart(playerid,playerid2);
	return 1;
}
SetTimerEx("Drift",1000,0,"iii",playerid,cd-1,playerid2);
return 1;
}

