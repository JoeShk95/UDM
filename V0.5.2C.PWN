  GetPlayerPos(playerid, x, y, z);
  SpeedHack[playerid][0] = x;
  SpeedHack[playerid][1] = y;
  if(SpeedHackSpeed[playerid] > 36 && z < 45.0) SpeedHack_Warn[playerid][0] ++;
