	    LACY_GiveMoney(Rumble[playerid1],Rumble[bet_Rumble]);
	    ResetRumbleArray();
        KillTimer(Rumble_Timer);
	}

	if (Rumble[time] >= 300)
	{
        TogglePlayerControllable(Rumble[playerid1],true);
        TogglePlayerControllable(Rumble[playerid2],true);
        SpawnPlayer(Rumble[playerid1]);
        SpawnPlayer(Rumble[playerid2]);
