function(ClanWar_CountDown(Count_WarClan))
{
  if(!ClanWarOn) return 0;
  new string[256];
  format(string,sizeof string,"~y~Tournament~W~: ~p~%d",Count_WarClan);
  GameTextForAll(string,1000,4);
  Count_WarClan--;
  if(Count_WarClan > 0) SetTimerEx("ClanWar_CountDown",1000,0,"i",Count_WarClan);
  if(Count_WarClan < 2)
  {
	  SetTimer("ClanWar_End",WarTime*60*1000,0);
	  SendClientMessageToAll(COLOR_WHITE,"  ");
	  SendClientMessageToAll(COLOR_WHITE,"  ");
	  SendClientMessageToAll(COLOR_WHITE,"  ");
	  SendClientMessageToAll(COLOR_BLACK,"________________________________________________________________");
	  SendClientMessageToAll(0xFF66FFAA,".מלחמת-הקלאנים החלה, שימו לב, המלחמה תגמר עוד-10 דקות, הקלאן שיהיה לו הכי הרבה הריגות-ינצח");
	  SendClientMessageToAll(0xFF66FFAA,":הקלאנים שמתחרים זה-בזה");
      format(string,sizeof string,"1. %s | 2. %s | 3. %s | 4. %s",Clans[0],Clans[1],Clans[2],Clans[3]);
      SendClientMessageToAll(0x16EB43FF,string);
      SendClientMessageToAll(0xFF66FFAA,":פקודות מערכת");
      SendClientMessageToAll(COLOR_YELLOW,"/CState - בדיקת-מאזן ההריגות");
      SendClientMessageToAll(COLOR_YELLOW,"/CWatch - צפיה במלחמת הקלאנים");
      SendClientMessageToAll(COLOR_BLACK,"________________________________________________________________");
      ClanWarOn = 2;
      for(new i = 0; i < GetMaxPlayers(); i++) if(InClanWar[i]) ClanWar_SetPos(i,ClanCountWar[i]);
  }
  else if(Count_WarClan < 41)
  {
     for(new i = 0; i < GetMaxPlayers(); i++)
     {
	    if(!strcmp(dini_Get(GetPlayerFile(i),"Clan"),Clans[0],true) || !strcmp(dini_Get(GetPlayerFile(i),"Clan"),Clans[1],true) || !strcmp(dini_Get(GetPlayerFile(i),"Clan"),Clans[2],true) || !strcmp(dini_Get(GetPlayerFile(i),"Clan"),Clans[3],true))
	    {
			if(!InClanWar[i])
			{
                InClanWar[i] = 1;
			    TotalPlayersClanWar++;
	            format(string,sizeof string,".%d :המערכת צירפה אותך לקרב קלאנים, אתה משתתף מספר",TotalPlayersClanWar);
	            SendClientMessage(i,0xFF66FFAA,string);
	            format(string,sizeof string,".%s - הקלאן שלך הוא",dini_Get(GetPlayerFile(i),"Clan"));
	            SendClientMessage(i,0xFF66FFAA,string);
	            ResetVarPlayers(i);
	            if(!strcmp(dini_Get(GetPlayerFile(i),"Clan"),Clans[0],true)) ClanCountWar[i] = 0;
			    else if(!strcmp(dini_Get(GetPlayerFile(i),"Clan"),Clans[1],true)) ClanCountWar[i] = 1;
			    else if(!strcmp(dini_Get(GetPlayerFile(i),"Clan"),Clans[2],true)) ClanCountWar[i] = 2;
			    else if(!strcmp(dini_Get(GetPlayerFile(i),"Clan"),Clans[3],true)) ClanCountWar[i] = 3;
			}
         }
      }
   }
  return 1;
}

function(ClanWar_SetPos(playerid,count_team))
{
  if(count_team == -1) return 0;
  new rand = random(sizeof War_Clan1);
  if(count_team == 0) return SetPlayerPos(playerid,War_Clan1[rand][0], War_Clan1[rand][1], War_Clan1[rand][2]);
  else if(count_team == 1) return SetPlayerPos(playerid,War_Clan2[rand][0], War_Clan2[rand][1], War_Clan2[rand][2]);
  else if(count_team == 2) return SetPlayerPos(playerid,War_Clan3[rand][0], War_Clan3[rand][1], War_Clan3[rand][2]);
  else if(count_team == 3) return SetPlayerPos(playerid,War_Clan4[rand][0], War_Clan4[rand][1], War_Clan4[rand][2]);
  return 1;
}

function(ClanWar_End())
{
    if(!ClanWarOn) return 0;
	new string[128];
	if((ClanKills[0] == ClanKills[1] || ClanKills[0] == ClanKills[2] || ClanKills[0] == ClanKills[3])
	|| (ClanKills[1] == ClanKills[0] || ClanKills[1] == ClanKills[2] || ClanKills[1] == ClanKills[3])
	|| (ClanKills[2] == ClanKills[0] || ClanKills[2] == ClanKills[1] || ClanKills[2] == ClanKills[3])
	|| (ClanKills[3] == ClanKills[0] || ClanKills[3] == ClanKills[1] || ClanKills[3] == ClanKills[2]))
	{
