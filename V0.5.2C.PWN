forward StartMission(playerid);
public StartMission(playerid)
{

	if(Pack[playerid] == 3) return SendClientMessage(playerid, COLOR_WHITE, ".אי אפשר לבצע את המשימה מעל 3 פעמים"),0;
	for(new i; i<200; i++)
	{
	    if(IsPlayerConnected(i))
	    {
	        if(InMission[i] == 1)
	        {
		        SendClientMessage(playerid, COLOR_WHITE, ".אתה לא יכול/ה לעשות את המשימה כי מישהו אחר עושה אותה, שהוא יסיים תחזור לפה");
		        break;
			}
		}
	}
	if(GetOnlinePlayers() < 10 && !IsPlayerXAdmin(playerid,5)) return SendClientMessage(playerid,COLOR_WHITE,".אי אפשר לבצע את המשימה עם פחות מ-5 אנשים בשרת");
    SendFormatMsg(-1,0xcccc00ff,"! התחיל במשימת הכסף הגנוב ,%s",GetName(playerid));
    SendClientMessageToAll(0xcccc00ff, "!יש לכם 5 דקות לעצור בעדו, מי שיעצור בעדו, יזכה בפרס של 3000$, קדימה");
    SendClientMessage(playerid, COLOR_GREEN, ",יש לך 5 דקות לבצע את המשימה, כל עוד אתה נמצא במשימה, אתה לא יכול/ה להשתגר");
    SendClientMessage(playerid, COLOR_GREEN, ".המיקום שאתה צריך להעביר אליו את הכסף - תחנת הרכבת הנטושה, נמצא ליד מוסך וואנג");
	SendClientMessage(playerid, COLOR_YELLOW, "/QuitMission - על מנת לפרוש מהמשימה");
    CutMissionTimer[playerid] = SetTimerEx("CutMission", 1000*60*5, 1, "i", playerid);
    InMission[playerid] = 1; DidMission[playerid] = 1;
    Pack[playerid]++;
    DestroyPickup(pickupMission);
    SetPlayerColor(playerid,COLOR_BLACK);
	return 1;
}
