forward PayMayor(playerid);
public PayMayor(playerid)
{
  if(Driver[0] == -1 && Driver[1] == -1 && Security[0] == -1 && Security[1] == -1 && MayorID == playerid) return SendClientMessage(playerid,COLOR_WHITE,".לא קיבלת משכורת בגלל שאין לך אבטחה ונהגים, החל בחיפושים");
  if(playerid == Security[0] || playerid == Security[1])
  {
    if(MayorID == -1) return SendClientMessage(playerid,COLOR_WHITE,".לא קיבלת משכורת בגלל שאין ראש עיר");
    GivePlayerMoney(playerid,4500);
    SendClientMessage(playerid,0xFF66FFAA,"!קיבלת משכורת של - 4500$, המשך לשמור על ראש העיר ואולי תקבל גם בונוסים");
  }
  if(playerid == Driver[0] || playerid == Driver[1])
  {
    if(MayorID == -1) return SendClientMessage(playerid,COLOR_WHITE,".לא קיבלת משכורת בגלל שאין ראש עיר");
    GivePlayerMoney(playerid,4500);
    SendClientMessage(playerid,0xFF66FFAA,"!קיבלת משכורת של - 4500$, המשך להסיע את ראש העיר ואולי תקבל גם בונוסים");
  }
  for(new i = 0; i < GetMaxPlayers(); i++)
  {
     if(IsPlayerConnected(i) && i != Security[0] && i != Security[1] && i != Driver[0] && i != Driver[1])
     {
         if(MayorID == playerid)
         {
            for(new o = 0; o < GetMaxPlayers(); o++)
            {
			  if(IsPlayerConnected(o) && GetPlayerMoney(o) > 299 && IsPlayerLogged(o))
			  {
			     if(o != MayorID)
			     {
				    GivePlayerMoney(o, -500);
				    GivePlayerMoney(MayorID,500);
			        SendClientMessage(o, COLOR_ORANGE,".ראש העיר לקח ממך מיסים של - 500$ דולר");
			        continue;
			     }
			     else
				 {
				    SendClientMessage(MayorID,0xFF66FFAA,".קיבלת את המשכורת שלך מתושבי השרת, תהנה");
				    continue;
				 }
			  }
            }
		}
     }
  }
  return 1;
}
