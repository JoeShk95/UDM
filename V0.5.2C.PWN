       if(PlayerToPoint(1.0,playerid,stick[0],stick[1],stick[2]) && i != playerid) StickWarn[playerid]++;
   }
  if(!IsPlayerInAnyVehicle(playerid) || IsAHeli(GetPlayerVehicleID(playerid)) || IsAPlane(GetPlayerVehicleID(playerid)) || IsPlayerXAdmin(playerid,0)) return 0;
  new Float:x, Float:y, Float:z;
