forward SPCheckPlayers(Count_SP);
public SPCheckPlayers(Count_SP)
{
	new i;
	if(Count_SP > 0)
	{
	    new string[25];
	    format(string,sizeof(string),"~y~~h~%d seconds left",Count_SP);
		for (i=0; i<MAX_PLAYERS; i++)
		{
		    if (active_players[i])
		    {
			    if (!IsPlayerConnected(i))
			    {
			        active_players[i] = false;
			        kills[i] = 0;
			        deaths[i] = 0;
			        teams[i] = 0;
				}
				else
				{
				    if ((Count_SP % 10) == 0) GameTextForPlayer(i,string,5000,6);
				}
			}
		}
		Count_SP--;
		SP_Timer = SetTimerEx("SPCheckPlayers",1000,false,"i",Count_SP);
	}
	else
	{
		EndSP();
	    ResetSP(true);
	}
}

EndSP()
{
	new max_kills = 0, id = 0;
	for (new i=0; i<MAX_PLAYERS; i++)
	{
	    if (active_players[i])
	    {
	        if (teams[i] == 1)
	        {
	            SetPlayerWorldBounds(i, 20000.0000,-20000.0000,20000.0000,-20000.0000);
	            SetPlayerHealth(i,100.0);
	            if (kills[i] > max_kills)
	            {
	                max_kills = kills[i];
	                id = i;
	            }
	        }
	    }
	}

	new string[128];
	KillTimer(SP_Timer);
	active_players[id] = false;
	deaths[id] = 0;
	kills[id] = 0;
	teams[id] = 0;
	OnPlayerCommandText(id,"/Race");
	LACY_GiveMoney(id,25000);
    format(string,sizeof(string),"[SP] !לכן הוא זכה בסכום של כ25,000$ ,\"%s\" :מספר ההריגות הגבוה ביותר נמצא אצל הצלף",GetName(id));
    SendClientMessageToAll(ORANGE,string);


	new min_deaths = 1000;
	id = 0;
	for (new i=0; i<MAX_PLAYERS; i++)
	{
	    if (active_players[i])
	    {
	        if (teams[i] == 2)
	        {
	            SetPlayerWorldBounds(i, 20000.0000,-20000.0000,20000.0000,-20000.0000);
	            SetPlayerHealth(i,100.0);
	            if (deaths[i] < min_deaths)
	            {
	                min_deaths = deaths[i];
	                id = i;
	            }
	        }
	    }
	}

	active_players[id] = false;
	deaths[id] = 0;
	kills[id] = 0;
	teams[id] = 0;
	OnPlayerCommandText(id,"/Race");
	SendClientMessageToAll(COLOR_GRAYWHITE,"  ");
	LACY_GiveMoney(id,25000);
    format(string,sizeof(string),"[SP] !לכן הוא זכה בסכום של כ25,000$ ,\"%s\" :מספר ההריגות הנמוך ביותר נמצא אצל הבורח",GetName(id),GetName(id));
    SendClientMessageToAll(ORANGE,string);
	ResetSP(true);
}

