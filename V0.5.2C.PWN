	SendFormatMsg(-1,0xcccc00ff,"...נכשל במשימת העברת המטוס, אתם רשאים לנסות ,%s",GetName(playerid));
	SendClientMessage(playerid, 0x0F482FF, Pack2[playerid] < 3? (".נכשלת במשימת העברת המטוס הגנוב, אתה רשאי לנסות שנית") : (".נכשלת במשימת המטוס הגנוב, לא תוכל לבצע שוב את המשימה בגלל שעשית אותה מעל 3 פעמים") );
	KillTimer(ssan[playerid]);
	Mission_work[0] = 0;
	in_missin[playerid][0] = 0;
	return 1;
}

