function(EndClanWar())
{
  TotalPlayersClanWar = 0, ClanWarOn = 0;
  for(new i = 0; i < GetMaxPlayers(); i++) if(IsPlayerConnected(i)) InClanWar[i] = -1;
  return 1;
}
stock ResetVarPlayers(playerid)
{
issniper[playerid] = 0;
if(playerid == BID[0] && Using_Fight == 2) SetPlayerHealth(playerid,0.0);
if(playerid == BID[1] && Using_Fight == 2) SetPlayerHealth(playerid,0.0);
if(playerid == WID_W[0] && Using_wwe == 2) SetPlayerHealth(playerid,0.0);
if(playerid == WID_W[1] && Using_wwe == 2) SetPlayerHealth(playerid,0.0);
if(active_players[playerid] == true) SetPlayerHealth(playerid,0.0);
if(isinPhyro2[playerid]) SetPlayerHealth(playerid,0.0);
if(InGlass[playerid] == 2) SetPlayerHealth(playerid,0.0);
if(Pinvaderin[playerid] == 1) SetPlayerHealth(playerid,0.0);
if(InExplosion[playerid] == 1) SetPlayerHealth(playerid,0.0);
if(InCounterStrike[playerid] > 0) InCounterStrike[playerid] = 0, SetPlayerHealth(playerid,0.0);
if(Pjetpackin[playerid] == 1) SetPlayerHealth(playerid,0.0), Pjetpackin[playerid] = 0;
if(Youregisterrace[playerid] ==1) SetPlayerHealth(playerid,0.0), Youregisterrace[playerid] = 0;
if(YouRegister[playerid] ==1) YouRegister[playerid] = 0, SetPlayerHealth(playerid,0.0);
if(Registertrs[playerid] ==1) Registertrs[playerid] = 0;
if(KK[playerid] == 1) SetPlayerHealth(playerid,0.0), KK[playerid] = 0;
if(MonsterNoTeleportion[playerid] == 1) MonsterNoTeleportion[playerid] = 0, SetPlayerHealth(playerid,0.0);
if(PGrein[playerid] == 1) SetPlayerHealth(playerid,0.0);
if(PPw[playerid] == 1) SetPlayerHealth(playerid,0.0);
if(PP[playerid] == 1) SetPlayerHealth(playerid,0.0);
if(PPr[playerid] == 1) SetPlayerHealth(playerid,0.0);
if(CtfpInfo[playerid][GreenTeam] || CtfpInfo[playerid][RedTeam]) CtfpInfo[playerid][RedTeam] = 0, CtfpInfo[playerid][GreenTeam] = 0;
if((Rumble[playerid1] == playerid || Rumble[playerid2] == playerid) && Rumble[accept] == true) SetPlayerHealth(playerid,0.0);
return 1;
}

