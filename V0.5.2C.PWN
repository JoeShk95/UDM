public CheckAcceptPlayer()
{
	Rumble[time]++;
	if (!Rumble[accept] && (Rumble[time] == 30))
	{
	    TogglePlayerControllable(Rumble[playerid1],true);
	    SendClientMessage(Rumble[playerid1],red,".אף אחד לא אישר את ההזמנה שלך");
