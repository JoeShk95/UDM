        SendClientMessage(Rumble[playerid2],red,".הזמן הכולל של הקרב עבר את המקסימום");
        ResetRumbleArray();
        KillTimer(Rumble_Timer);
	}

	if (!IsPlayerConnected(Rumble[playerid1]))
	{
        TogglePlayerControllable(Rumble[playerid2],true);
        SendClientMessage(Rumble[playerid1],red,".ניצחת ניצחון טכני");
		SpawnPlayer(Rumble[playerid2]);
		LACY_GiveMoney(Rumble[playerid2],2*Rumble[bet_Rumble]);
        ResetRumbleArray();
        KillTimer(Rumble_Timer);
	}

	if (!IsPlayerConnected(Rumble[playerid2]))
	{
        TogglePlayerControllable(Rumble[playerid1],true);
        SendClientMessage(Rumble[playerid1],red,".ניצחת ניצחון טכני");
