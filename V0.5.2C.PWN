forward jetpackck();
public jetpackck()
{
if(jetpackStart == 1)
{
if(jetpackPlayers == 1)
{
for(new i = 0;i<MAX_PLAYERS;i++)
{
if(Injetpack[i] == 1)
{
jetpackWinner = i; jetpackEnd(1);
}
}
}
}
return 1;
}

