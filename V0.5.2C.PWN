function(RepairRentCar(playerid))
{
   TogglePlayerControllable(playerid,1);
   GetVehiclePos(RentInfo[playerid][CarID],RentInfo[playerid][vPosv][0],RentInfo[playerid][vPosv][1],RentInfo[playerid][vPosv][2]);
