stock SetPrice(vid)
{
   if(IsAPlane(vid)) PriceRentVehicle[vid] = 6000;
   if(IsABoat(vid)) PriceRentVehicle[vid] = 5000;
   if(IsAMoto(vid)) PriceRentVehicle[vid] = 8000;
   if(!IsAPlane(vid) && !IsABoat(vid) && !IsAMoto(vid)) PriceRentVehicle[vid] = 8000;
   return 1;
}

